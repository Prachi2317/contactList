{
    "version": 2,
    "name": "nodejs-express",
    "build": {
        "env": {
          "NODE_ENV": "production"
        },
        "command": "npm run build",
        "output": "build"
      },
      "routes": [
        {
          "src": "/(.*)",
          "dest": "./routes/index.js"
        }
      ]
    }